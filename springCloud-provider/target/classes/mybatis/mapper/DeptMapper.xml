<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gene.mapper.DeptMapper">


    <insert id="addDept" parameterType="org.gene.pojo.Dept">
        insert into dept (name) values (#{name})
    </insert>

    <select id="slel" parameterType="int" resultType="org.gene.pojo.Dept">
        select * from dept where deptNum = #{id}
    </select>
</mapper>